<h1><img src="/assets/stackoverflow_design.png">Tara Overflow</h1>

<ul>
<% @questions.each do |question| %>
  <li><%= link_to question.title, question_path(question) %></li>
  <% end %>
</ul>

<% if current_user != nil %>
  <%= link_to "Add New Question", new_question_path, :class => "btn btn-primary" %>
<% end %>
